{
    "files.exclude": {
        "**/.git": true,
        "**/build/*.[^p]*": true,
        "**/build/*.ph*": true,
        "**/csv/**/*.csv" : false,
        "**/csv/**/*.coeff.txt" : true,
    },
    "files.associations": {
        "*.tikz" : "latex"
    },
    "latex-workshop.latex.outDir": "%DIR%/build",
    "latex-workshop.latex.autoBuild.run": "never",
    "editor.wordWrap": "on",
    "latex-workshop.view.pdf.viewer": "tab",
}